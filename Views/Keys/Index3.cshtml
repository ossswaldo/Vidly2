@model IEnumerable<Vidly2.Models.ViblyyKeyy>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

  

}

    <li>
        <div class="jumbotron">
            <h1>CMC Demo ASP.NET</h1>
            <p class="lead">Please use link underneath to navigate faster</p>
            <div class="col-md-4">
                <p><a onclick="@("window.location.href='" + @Url.Action("Index", "Users") + "'");" class="btn btn-primary btn-lg">Users &raquo;</a></p>
            </div>
            <div class="col-md-4">
                <p><a onclick="@("window.location.href='" + @Url.Action("Create", "Users") + "'");" class="btn btn-primary btn-lg">Add User &raquo;</a></p>
            </div>
            <div class="col-md-4">
                <p><a onclick="@("window.location.href='" + @Url.Action("Create", "Keys") + "'");" class="btn btn-primary btn-lg">Add Keys &raquo;</a></p>
            </div>

            <p class="lead"> </p>
        </div>

        
    </li>


<p>
    @Html.ActionLink("Create New", "Create")
</p>
@using (Html.BeginForm("Index", "Keys", FormMethod.Get))
{
    @Html.TextBox("searching")
    <input type="submit" value="submit"/>
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpirationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Environment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastNotifiedDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpirationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Environment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comments)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastNotifiedDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>

